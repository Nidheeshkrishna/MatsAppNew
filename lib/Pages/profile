AddCoverImage(105272, coverpath)
                                                .then((value) =>
                                                    coverstatus = value!.result)
                                                .whenComplete(() => {
                                                      if (coverstatus == 1)
                                                        {
                                                          setState(() {
                                                            loading = !loading;
                                                          })
                                                        }
                                                      else
                                                        {
                                                          Fluttertoast.showToast(
                                                              msg:
                                                                  "Something went wrong",
                                                              toastLength: Toast
                                                                  .LENGTH_SHORT,
                                                              gravity:
                                                                  ToastGravity
                                                                      .CENTER,
                                                              timeInSecForIosWeb:
                                                                  1,
                                                              backgroundColor:
                                                                  Colors.red,
                                                              textColor:
                                                                  Colors.white,
                                                              fontSize: 16.0),
                                                        }
                                                    }),
                                            AddPanCardImage(105272, pancardpath)
